        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "      self.name = name\n",
        "      self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "      print(\"名前\", self.name)\n",
        "      print(\"年齢\", self.age)\n",
        "\n",
        "human1 = Human(\"太郎\", 20)\n",
        "\n",
        "human1.printinfo()\n"
